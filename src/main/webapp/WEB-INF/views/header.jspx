<div id="header" class="pull-left" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:spring="http://www.springframework.org/tags" version="2.0">
  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:output omit-xml-declaration="yes" />

      <spring:url var="logo_gvnix" value="/resources/images/logo_gvnix.png" />
      <spring:url var="home" value="/" />
      <spring:message code="button_home" var="home_label" htmlEscape="false" />

			<div class="pull-left brand">
        <a href="${home}" name="${fn:escapeXml(home_label)}"
        title="${fn:escapeXml(home_label)}">
				  <img src="${logo_gvnix}" />
        </a>
			</div>
</div>




